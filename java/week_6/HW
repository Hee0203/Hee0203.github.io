package org.javaro.lecture;

public class Test5_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("임희진"+"-"+"20191010"+"-"+"배열");
		int max = 30;
		int [] prime = new int [max];
		
		for(int i = 2; i < max; i++) {
			if(prime[i] == 1)continue;
			int j = i;
			while((j+=i)<max)
				prime[j] = 1;
		}
		for(int i = 2; i < max; i++) {
			if(prime[i] == 0)
				System.out.print(i + " ");   
		}
	}

}
