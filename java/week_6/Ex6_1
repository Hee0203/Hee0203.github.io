package org.javaro.lecture;

public class Ex6_1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("임희진"+"-"+"20191010"+"-"+"함수");
		int d = sum(1,2,3);
		System.out.println("d = "+d);
	}
	static int sum(int a, int b, int c) {
		int total = a+b+c;
		return total;
	}
}
